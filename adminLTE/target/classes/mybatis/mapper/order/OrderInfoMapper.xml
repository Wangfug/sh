  <select id="findOrderList" resultMap="BaseResultMap"  parameterType="java.util.Map">
    select
    o.order_no,o.time_start,o.time_end,o.state,c.name,c.mobile_phone
    from order_info o left join customer c
    <where>
      <if test="customer != null" >
        and c.CUSTOMER  LIKE CONCAT(CONCAT('%', #{customer}), '%')
      </if>
      <if test="state != null" >
        and o.STATE = #{state}
      </if>
      <if test="order_no != null" >
        and o.ORDER_NO = #{orderNo,jdbcType=VARCHAR}
      </if>
    </where>

  </select>
  <insert id="save" parameterType="com.lte.admin.order.entity.OrderInfo" >
    insert into order_info ( CREATE_BY, CREATE_TIME,
      CAR_ID
      )
    values (#{createBy,jdbcType=BIGINT}, #{createTime,jdbcType=TIMESTAMP},
      #{carId,jdbcType=BIGINT}
      )
  </insert>
